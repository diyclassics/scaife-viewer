{
  "name": "scaife-viewer",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf static/dist && mkdir -p static/dist/js && mkdir -p static/dist/css && mkdir -p static/dist/images",
    "build:dev": "npm run clean && webpack --config static/webpack.dev.js",
    "build:prod": "npm run clean && webpack --config static/webpack.prod.js",
    "dev": "npm run clean && concurrently --kill-others-on-fail \"python manage.py runserver\" \"webpack-dev-server --config static/webpack.dev.js\"",
    "heroku-postbuild": "npm run build:prod"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0-beta",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "cssnano-cli": "^1.0.5",
    "document-register-element": "^1.7.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^3.13.1",
    "exorcist": "^0.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "lodash.debounce": "^4.0.8",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "popper.js": "^1.12.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglify-js": "^3.1.0",
    "vue": "^2.4.4",
    "vue-custom-element": "^1.3.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "vuex": "^2.4.1",
    "vuex-persistedstate": "^2.0.0",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": "~8.6.0"
  }
}
